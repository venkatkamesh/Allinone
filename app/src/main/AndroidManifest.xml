                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name=".NewShakeSensor">
            <intent-filter>
                <action android:name="com.venkatkamesh.allinone.NewShakeSensor" />

                <category android:name="android.intent.category.DEFAULT" />
